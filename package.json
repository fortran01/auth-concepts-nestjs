{
  "name": "auth-concepts-nestjs",
  "version": "1.0.0",
  "description": "NestJS Authentication Demo",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:auth": "jest --testPathPattern=test/auth --testPathIgnorePatterns=\\.e2e-spec\\.ts$",
    "test:auth:e2e": "jest --config ./test/jest-e2e.json --testPathPattern=digest-auth.e2e-spec.ts",
    "generate-hash": "node src/scripts/generate-hash.js",
    "test:digest": "node src/scripts/test-digest-auth.js"
  },
  "keywords": [
    "nestjs",
    "authentication",
    "basic-auth"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^16.0.0",
    "bcrypt": "^5.1.0",
    "hbs": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cheerio": "^0.22.34",
    "@types/jest": "^29.5.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
